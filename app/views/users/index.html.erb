<div class="col-md-6">
  <h3>Current Users</h3>
  <table class='table table-striped table-bordered'>
    <% @users.each do |user| %>
      <tr>
        <td><strong><%= user.full_name %></strong></td>
        <td><%= link_to 'Submit New Readings', user_readings_path(user), id: "readings-link-#{user.id}" %></td>
        <td><%= link_to 'Run Report', new_user_report_path(user), id: "reports-link-#{user.id}" %></td>
       </tr>
    <% end %>
  </table>
</div>
